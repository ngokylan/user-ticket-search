    "rc-calendar": "9.15.11",
    "rc-time-picker": "^3.7.2",
    "react-dropzone": "11.3.1",
    "react-focus-lock": "^2.2.1",
    "react-popper": "^2.2.4",
    "react-select": "^4.1.0",
    "resize-observer": "^1.0.0"
  },
  "devDependencies": {
    "@babel/helper-define-map": "^7.12.13",
