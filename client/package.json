{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://server:8000",
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "webpack": "4.29.6",
    "cors": "^2.8.5",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll",
    "test-ci": "CI=true react-scripts test --env=jsdom --coverage --verbose",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts*\"",
    "start-mock-server": "node server/middlewares.ts",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "test-badges": "node_modules/.bin/istanbul-badges-readme",
    "test:e2e": "cypress open"
  },
  "devDependencies": {  
    "@types/axios": "0.14.0",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/i18n": "0.8.3",
    "@types/i18next": "12.1.0",
    "@types/i18next-browser-languagedetector": "2.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "10.12.12",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "6.0.10",
    "@types/react-router": "4.4.1",
    "@types/react-router-dom": "4.3.1",
    "@types/react-test-renderer": "16.0.3",
    "@types/reactstrap": "6.4.3",
    "@types/redux-form": "7.4.12",
    "@types/redux-mock-store": "^1.0.2",
    "@types/sanitize-html": "^1.23.3",
    "@types/uuid": "3.4.4",
    "@types/yup": "0.26.3",
    "cypress": "^6.5.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "^7.20.0",
    "istanbul-badges-readme": "^1.2.0",
    "jest-enzyme": "^7.1.2",
    "json-server": "^0.14.2",
    "moxios": "0.4.0",
    "nodemon": "^2.0.7",
    "react-app-rewire-typescript": "2.0.2",
    "react-test-renderer": "16.6.3",
    "redux-mock-store": "^1.5.4",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "html",
      "json-summary",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "testMatch": [
      "**/__tests__ /**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "pretty-quick --staged && yarn lint && yarn format && yarn test && yarn test-badges && git add ./README.md"
    }
  }
}
